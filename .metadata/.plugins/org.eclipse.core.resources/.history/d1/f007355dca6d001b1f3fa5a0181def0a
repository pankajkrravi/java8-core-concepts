
public class Loops {
	public static void main(String[] args) {
		// do while loop
		int count = 0;
		do {
			System.out.println(" %%%%%%% print 5 times %%%%%%%% ");
			count++;
		} while (count <= 4);

		// while loop
		while (count <= 4) {
			System.out.println(" ========== print 5 times ==========  ");
			count++;
		}

		// for loop
		for (int i = 0; i <= 4; i++) {
			System.out.println(" ********** Print 5 times ******** ");
		}
	}
}
